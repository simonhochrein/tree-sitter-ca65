{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ca65",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "macro"
              },
              {
                "type": "SYMBOL",
                "name": "constant"
              },
              {
                "type": "SYMBOL",
                "name": "line_comment"
              },
              {
                "type": "SYMBOL",
                "name": "section"
              },
              {
                "type": "SYMBOL",
                "name": "instruction"
              },
              {
                "type": "SYMBOL",
                "name": "_newline"
              }
            ]
          }
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_section_body"
            }
          }
        }
      ]
    },
    "_section_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "subsection"
        },
        {
          "type": "SYMBOL",
          "name": "instruction"
        },
        {
          "type": "SYMBOL",
          "name": "macro"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "subsection": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sublabel"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "subsection_body"
            }
          }
        }
      ]
    },
    "subsection_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "instruction"
        },
        {
          "type": "SYMBOL",
          "name": "macro"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opcode"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "operands"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "constant": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "operand"
        }
      ]
    },
    "opcode": {
      "type": "PATTERN",
      "value": "[a-z]{3}"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_-]+"
    },
    "operands": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "operand"
            },
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "SYMBOL",
              "name": "operand"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "operand"
        }
      ]
    },
    "operand": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "immediate"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "sublabel": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\@[A-Za-z0-9_-]+"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "macro": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "SYMBOL",
              "name": "operands"
            },
            {
              "type": "SYMBOL",
              "name": "_newline"
            }
          ]
        }
      ]
    },
    "include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ".include"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "immediate": {
      "type": "PATTERN",
      "value": "\\$?[0-9A-Fa-f]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"(.*)\""
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PATTERN",
            "value": ".*"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "PATTERN",
            "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    },
    "_newline": {
      "type": "STRING",
      "value": "\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": " |\\t|\\r"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    }
  ],
  "conflicts": [
    [
      "subsection"
    ],
    [
      "section"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [
    "_newline"
  ],
  "supertypes": []
}
