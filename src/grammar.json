{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ca65",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "macro"
              },
              {
                "type": "SYMBOL",
                "name": "constant"
              },
              {
                "type": "SYMBOL",
                "name": "line_comment"
              },
              {
                "type": "SYMBOL",
                "name": "section"
              },
              {
                "type": "SYMBOL",
                "name": "instruction"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "subsection"
              },
              {
                "type": "SYMBOL",
                "name": "instruction"
              },
              {
                "type": "SYMBOL",
                "name": "macro"
              }
            ]
          }
        }
      ]
    },
    "subsection": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sublabel"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "instruction"
              },
              {
                "type": "SYMBOL",
                "name": "macro"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        }
      ]
    },
    "instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opcode"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "operands"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "constant": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "operand"
        }
      ]
    },
    "opcode": {
      "type": "PATTERN",
      "value": "[a-z]{3}"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_-]+"
    },
    "operands": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "operand"
            },
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "SYMBOL",
              "name": "operand"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "operand"
        }
      ]
    },
    "operand": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "immediate"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "immediate": {
      "type": "PATTERN",
      "value": "\\$?[0-9]+"
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "sublabel": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\.[A-Za-z0-9_-]+"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "operands"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PATTERN",
            "value": ".*"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "PATTERN",
            "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": " |\\t|\\r"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    }
  ],
  "conflicts": [
    [
      "subsection"
    ],
    [
      "section"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
